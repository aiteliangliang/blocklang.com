{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"title": "Api repo configuration",
	"type": "object",
	"required": [
		"name",
		"category"
	],
	"properties": {
		"name": {
			"type": "string",
			"description": "组件库名称",
			"maxLength": 60,
			"pattern": "^[a-zA-Z0-9\\-\\w]+$"
		},
		"displayName": {
			"type": "string",
			"description": "组件库的显示名",
			"maxLength": 60
		},
		"description": {
			"type": "string",
			"description": "组件库的详细介绍",
			"maxLength": 500
		},
		"category": {
			"type": "string",
			"description": "组件库的种类",
			"enum": [
				"Widget",
				"Service",
				"WebApi"
			]
		}
	}
}